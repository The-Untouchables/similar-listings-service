{
  "name": "similar-listings-service",
  "version": "1.0.0",
  "description": "Similar listings service",
  "main": "client/dist/app.js",
  "scripts": {
    "dev": "webpack --d --watch",
    "start": "node server/index.js",
    "seed": "node db/index.js",
    "deploy": "npm run seed && npm run start",
    "test": "jest"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "unmockedModulePathPatterns": [
      "react",
      "enzyme",
      "jest-enzyme"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/The-Untouchables/similar-listings-service.git"
  },
  "keywords": [
    "services",
    "javascript",
    "reactjs",
    "express",
    "nodejs",
    "docker",
    "microservice",
    "mongodb"
  ],
  "author": "Nyah Check",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/The-Untouchables/similar-listings-service/issues"
  },
  "homepage": "https://github.com/The-Untouchables/similar-listings-service#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.3",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "bluebird": "^3.5.1",
    "chai-as-promised": "^7.1.1",
    "express": "^4.16.2",
    "jquery": "^3.3.1",
    "mongoose": "^5.0.6",
    "node-vibrant": "^3.0.0",
    "nodemon": "^1.17.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-slick": "^0.19.0",
    "react-star-rating-component": "^1.3.0",
    "slick-carousel": "^1.8.1",
    "underscore": "^1.8.3",
    "webpack": "^3.11.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "css-loader": "^0.28.10",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest": "^22.4.0",
    "jest-enzyme": "^4.2.0",
    "sinon": "^4.4.0",
    "style-loader": "^0.20.2"
  }
}
